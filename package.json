{
  "name": "full-stack-app",
  "version": "0.0.0",
  "description": "Template full-stack JavaScript app",
  "main": "server.js",
  "scripts": {
    "start": "babel server.js -o es5server.js && node es5server.js",
    "build": "mkdir -p build && cp client/*.css client/*.html build/ && webpack",
    "watch": "npm run build && run-p watch:*",
    "watch:copy": "chokidar \"client/*.css\" \"client/*.html\" -c \"cp {path} build/\"",
    "watch:js": "webpack --watch",
    "watch:server": "nodemon -w server.js server.js -x \"npm run\"",
    "server.js": "npm start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rosuav/full-stack-app.git"
  },
  "author": "Chris Angelico (cangelico@thinkful.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Rosuav/full-stack-app/issues"
  },
  "homepage": "https://github.com/Rosuav/full-stack-app#readme",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "webpack": "^1.13.3"
  }
}
